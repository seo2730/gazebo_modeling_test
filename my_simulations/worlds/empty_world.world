<?xml version="1.0" encoding="UTF-8"?>
<sdf version="1.5">
  <world name = "default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri> 
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://my_model</uri>
      <static>false</static>
      <!-- x,y,z,roll,pitch,yaw -->
      <pose>0 0 5 0 0 0</pose>
    </include>
  </world>
</sdf>
